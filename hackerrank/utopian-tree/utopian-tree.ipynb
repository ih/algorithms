      "def treeHeight(cycles):\n",
      "    current_cycle = True\n",
      "    height = 1\n",
      "    for i in range(cycles):\n",
      "        if current_cycle:\n",
      "            height *= 2\n",
      "        else:\n",
      "            height += 1\n",
      "        current_cycle = not current_cycle\n",
      "    return height"
     ],
